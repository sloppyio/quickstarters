{
    "project":"gogs",
    "services":[
        {
            "id":"frontend",
            "apps":[
                {
                    "id":"gogs",
                    "domain":{
                        "type":"HTTP",
                        "uri":"$URI"
                    },
                    "mem":512,
                    "image":"gogs/gogs",
                    "instances":1,
                    "volumes":[
                        {
                            "container_path":"/data",
                            "size":"8GB"
                        }
                    ],
                    "port_mappings":[
                        {
                            "container_port":3000
                        }
                    ]
                }
            ]
        }
    ]
}
